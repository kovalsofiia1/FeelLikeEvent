{
  "expo": {
    "name": "EventsApp",
    "slug": "EventsApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/feellikeevent.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.event.foryou"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/feellikeevent.jpg"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-image-picker"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}